{
	"cSpell.customDictionaries": {
		"package-dictionary": {
			"name": "package dictionary",
			"path": "./.vscode/.cspell/package-dictionary.txt",
			"addWords": true,
			"scope": "workspace"
		},
		"comicinfo-dictionary": {
			"name": "ComicInfo.xml dictionary",
			"path": "./.vscode/.cspell/comicinfo-dictionary.txt",
			"addWords": true,
			"scope": "workspace"
		}
	},
	"cSpell.languageSettings": [
		{ "languageId": "go", "allowCompoundWords": false, "dictionaries": ["go"] },
		{ "languageId": "typescriptreact", "dictionaries": ["typescript", "node"] },
		{ "languageId": "css", "dictionaries": ["fonts", "css"] }
	],
	"cSpell.ignorePaths": [
		"package-lock.json",
		"node_modules",
		"vscode-extension",
		".git/objects",
		".vscode",
		".vscode-insiders",
		"wailsjs"
	],
	"editor.formatOnSave": true,
	"editor.unicodeHighlight.includeStrings": false,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.tabSize": 4,
	"editor.detectIndentation": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.bracketSameLine": true,
	"prettier.singleAttributePerLine": false,
	"prettier.printWidth": 120,
	"code-runner.executorMap": {
		"go": "wails dev"
	},
	"code-runner.clearPreviousOutput": true,
	"code-runner.runInTerminal": true
}
